<!DOCTYPE html>
<html>
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<a href="/signout">Signout</a>
        
<br>
<br>

<a href="/show_contacts">View Contacts</a>
<a href="/upload">Upload Contacts</a>

<h2>Contacts</h2>

<table>
    <% contacts.forEach((cont)=>{ %>
        <tr>
          <th><%= cont.name %></th>
          <th><%= cont.phone %></th>
          <th><%= cont.email %></th>
          <th><%= cont.linkedIn %></th>
        </tr>
    <% }) %>
</table>

</body>
</html>

